{
    "name": "example-lint-commit",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "prettier:check": "prettier --config .prettierrc --check \"src/**/*.{js,ts,css,html,vue}\" --write",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --ignore-path .gitignore --fix"
    },
    "dependencies": {
        "vue": "^3.2.47",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^4.0.0",
        "eslint": "^8.38.0",
        "eslint-plugin-vue": "^9.10.0",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.2",
        "prettier": "^2.8.7",
        "vite": "^4.1.4"
    },
    "gitHooks": {
      "pre-commit": "lint-staged --allow-empty"
    },
    "lint-staged": {
        "*.{js,jsx,vue}": [
            "eslint --fix",
            "prettier --write",
            "git add"
        ]
    }
}
